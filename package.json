{
  "name": "artifact-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build-dev": "webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "getdata": "node src/data/update-collections-data.js",
    "getimgs": "node src/data/get-images-and-colors.js",
    "organize": "node src/data/reorganize-data-and-colors.js",
    "update": "npm run getdata && npm run getimgs && npm run organize && npm run build",
    "demo-start": "http-server -c1",
    "api-start": "node ./index.js",
    "start": "vercel dev",
    "vercel-build": "yum install libuuid-devel libmount-devel zlib && cp /lib64/{libuuid,libmount,libblkid,libz}.so.1 node_modules/canvas/build/Release/ && npm run build"
  },
